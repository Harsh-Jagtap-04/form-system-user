atch (error) {\r\n          console.error(error);\r\n          this.message = 'Invalid OTP.';\r\n        }\r\n      },\r\n      toggleForm() {\r\n        // Toggle between login and registration forms\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .login-page {\r\n    background: #FFFFFF;\r\n    width: 500px;\r\n\r\n    margin: auto;\r\n    text-align: center;\r\n  }\r\n  \r\n  .form {\r\n  position: relative;\r\n  z-index: 1;\r\n  background: #FFFFFF;\r\n  width: 100%; /* Set the width to 100% to fill the available space */\r\n  padding: 45px;\r\n  text-align: center;\r\n  box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.2), 0 5px 5px 0 rgba(0, 0, 0, 0.24);\r\n}\r\n\r\n/* Other styles for input, button, and message */\r\n.form input {\r\n  font-family: 'Roboto', sans-serif;\r\n  outline: 0;\r\n  background: #f2f2f2;\r\n  width: 100%;\r\n  border: 0;\r\n  margin: 0 0 15px;\r\n  padding: 15px;\r\n  box-sizing: border-box;\r\n  font-size: 14px;\r\n}\r\n.login-form {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);\r\n  width: 60%; /* Adjust the width as needed */\r\n  margin-right: 20px; /* Add space between LoginForm and SideTabs */\r\n}\r\n  \r\n  .form button {\r\n    font-family: 'Roboto', sans-serif;\r\n    text-transform: uppercase;\r\n    outline: 0;\r\n    background: #4CAF50;\r\n    width: 100%;\r\n    border: 0;\r\n    padding: 15px;\r\n    color: #FFFFFF;\r\n    font-size: 14px;\r\n    transition: all 0.3s ease;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .form button:hover,\r\n  .form button:active,\r\n  .form button:focus {\r\n    background: #43A047;\r\n  }\r\n  \r\n  .form .message {\r\n    margin: 15px 0 0;\r\n    color: #b3b3b3;\r\n    font-size: 12px;\r\n  }\r\n  \r\n  .form .message a {\r\n    color: #4CAF50;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  .form .register-form {\r\n    display: none;\r\n  }\r\n  \r\n  .container {\r\n    position: relative;\r\n  